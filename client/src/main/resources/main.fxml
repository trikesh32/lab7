<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="670.0" prefWidth="1080.0" style="-fx-background-color: #EFEFEF;" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.controllers.MainController">
    <children>
        <ComboBox fx:id="languageComboBox" layoutX="966.0" layoutY="14.0" prefHeight="27.0" prefWidth="118.0" style="-fx-background-radius: 10; -fx-border-radius: 10; -fx-border-color: #c0c0c0;" AnchorPane.rightAnchor="10" AnchorPane.topAnchor="16.0" />
        <Label fx:id="userLabel" layoutX="14.0" layoutY="18.0" text="Пользователь:" AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="20.0">
            <font>
                <Font name="Segoe UI" size="16.0" />
            </font>
        </Label>
        <AnchorPane layoutX="1.0" layoutY="58.0" prefHeight="590.0" prefWidth="956.0">
            <children>
                <FlowPane alignment="CENTER" columnHalignment="CENTER" hgap="10.0" layoutY="492.0" prefHeight="120.0" prefWidth="1080.0" vgap="10.0">
                    <Button fx:id="helpButton" mnemonicParsing="false" onAction="#help" prefHeight="30.0" prefWidth="90.0" style="-fx-background-radius: 10; -fx-border-radius: 10;" text="Help">
                        <font>
                            <Font name="Segoe UI" size="16.0" />
                        </font>
                    </Button>
                    <Button fx:id="infoButton" mnemonicParsing="false" onAction="#info" prefHeight="30.0" prefWidth="125.0" style="-fx-background-radius: 10; -fx-border-radius: 10;" text="Info">
                        <font>
                            <Font name="Segoe UI" size="16.0" />
                        </font>
                    </Button>
                    <Button fx:id="addButton" mnemonicParsing="false" onAction="#add" style="-fx-background-radius: 10; -fx-border-radius: 10;" text="Add">
                        <font>
                            <Font name="Segoe UI" size="16.0" />
                        </font>
                    </Button>
                    <Button fx:id="updateButton" mnemonicParsing="false" onAction="#update" style="-fx-background-radius: 10; -fx-border-radius: 10;" text="Update">
                        <font>
                            <Font name="Segoe UI" size="16.0" />
                        </font>
                    </Button>
                    <Button fx:id="removeByIdButton" mnemonicParsing="false" onAction="#removeById" prefHeight="30.0" prefWidth="153.0" style="-fx-background-radius: 10; -fx-border-radius: 10;" text="Remove by ID">
                        <font>
                            <Font name="Segoe UI" size="16.0" />
                        </font>
                    </Button>
                    <Button fx:id="clearButton" mnemonicParsing="false" onAction="#clear" style="-fx-background-radius: 10; -fx-border-radius: 10;" text="Clear">
                        <font>
                            <Font name="Segoe UI" size="16.0" />
                        </font>
                    </Button>
                    <Button fx:id="executeScriptButton" mnemonicParsing="false" onAction="#executeScript" style="-fx-background-radius: 10; -fx-border-radius: 10;" text="Execute script">
                        <font>
                            <Font name="Segoe UI" size="16.0" />
                        </font>
                    </Button>
                    <Button fx:id="removeLowerButton" mnemonicParsing="false" onAction="#removeLower" prefHeight="30.0" prefWidth="140.0" style="-fx-background-radius: 10; -fx-border-radius: 10;" text="Remove Lower">
                        <font>
                            <Font name="Segoe UI" size="16.0" />
                        </font>
                    </Button>
                    <Button fx:id="sumOfCapacityButton" mnemonicParsing="false" onAction="#sumOfCapacity" prefHeight="30.0" prefWidth="140.0" style="-fx-background-radius: 10; -fx-border-radius: 10;" text="Sum of capacity">
                        <font>
                            <Font name="Segoe UI" size="16.0" />
                        </font>
                    </Button>
                    <Button fx:id="filterByCapacityButton" mnemonicParsing="false" onAction="#filterByCapacity" prefHeight="30.0" style="-fx-background-radius: 10; -fx-border-radius: 10;" text="Filter by capacity">
                        <font>
                            <Font name="Segoe UI" size="16.0" />
                        </font>
                    </Button>
                    <Button fx:id="filterLessThanTypeButton" mnemonicParsing="false" onAction="#filterLessThanType" style="-fx-background-radius: 10; -fx-border-radius: 10;" text="Filter less than type">
                        <font>
                            <Font name="Segoe UI" size="16.0" />
                        </font>
                    </Button>
                    <Button fx:id="exitButton" mnemonicParsing="false" onAction="#exit" style="-fx-background-radius: 10; -fx-border-radius: 10;" text="Exit">
                        <font>
                            <Font name="Segoe UI" size="16.0" />
                        </font>
                    </Button>
                    <Button fx:id="logoutButton" mnemonicParsing="false" onAction="#logout" style="-fx-background-radius: 10; -fx-border-radius: 10;" text="Log Out">
                        <font>
                            <Font name="Segoe UI" size="16.0" />
                        </font>
                    </Button>
                </FlowPane>
                <TabPane layoutX="-1.0" prefHeight="492.0" prefWidth="1080.0" tabClosingPolicy="UNAVAILABLE">
                    <tabs>
                        <Tab fx:id="tableTab" text="Таблица">
                            <content>
                                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                                    <children>
                                        <TableView fx:id="tableTable" prefHeight="463.0" prefWidth="1080.0">
                                            <columns>
                                                <TableColumn fx:id="ownerColumn" prefWidth="117.0" text="Owner" />
                                                <TableColumn fx:id="idColumn" prefWidth="73.0" text="ID" />
                                                <TableColumn fx:id="nameColumn" prefWidth="73.0" text="Name" />
                                                <TableColumn prefWidth="58.0" text="X" fx:id="xColumn" />
                                                <TableColumn fx:id="yColumn" prefWidth="36.0" text="Y" />
                                                <TableColumn fx:id="dateColumn" prefWidth="133.0" text="Creation date" />
                                                <TableColumn fx:id="enginePowerColumn" prefWidth="151.0" text="Engine power" />
                                                <TableColumn fx:id="capacityColumn" prefWidth="156.0" text="Capacity" />
                                                <TableColumn fx:id="vehicleTypeColumn" prefWidth="141.0" text="Vehicle Type" />
                                                <TableColumn fx:id="fuelTypeColumn" prefWidth="141.0" text="Fuel Type" />
                                            </columns>
                                        </TableView>
                                    </children>
                                </AnchorPane>
                            </content>
                        </Tab>
                        <Tab fx:id="visualTab" text="Visualization">
                            <content>
                                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                                    <children>
                                        <AnchorPane fx:id="visualPane" prefHeight="463.0" prefWidth="1080.0" style="-fx-background-color: white;" />
                                    </children>
                                </AnchorPane>
                            </content>
                        </Tab>
                    </tabs>
                </TabPane>
            </children>
        </AnchorPane>
    </children>
</AnchorPane>
